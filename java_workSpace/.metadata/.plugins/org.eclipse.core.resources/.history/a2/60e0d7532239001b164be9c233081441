	public static void main(String[] args) {
		PrintfTest p = new PrintfTest();
		p.test1();
		
	}

	public void test1() {
		boolean bool = true;
		char ch = '헐';
		int i = 100;
		double dnum = 0.12345678;
		
		System.out.printf("%b이냐 %b이냐 %b로다!!!!",bool,!bool,bool);
		
		
		
