package com.kh.java.print;

public class PrintTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		PrintTest p = new PrintTest();
		p.test1();
	}
	
	/**
	 * escape 문자
	 * (\ + 특정문자)
	 * 
	 * \n 개행문자(윈도우 \r\n)
	 * \t 탭과 같은효과
	 * \\ 역슬래시를 출력할때
	 * \" 쌍따옴표 출력
	 * 
	 */
	
	public void test1() {
		System.out.println("안녕");
		System.out.print("하\n");
		System.out.print("하");
		System.out.print("하");
	}

}
